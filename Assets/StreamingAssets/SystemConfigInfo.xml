<?xml version="1.0" encoding="utf-8" ?>
<!--游戏项目整体配置参数-->
<SystemConfigInfo>
  <!--Log日志核心参数-->
  <!--日志路径-->
  <LogPath>D:\DungeonFighterLog.txt</LogPath>
  <!--日志的状态：开发模式 Develop（全部显示）/指定模式 Special（指定输出模式）/部署模式 Deploy(后台核心信息,用户反馈)/停止模式 Stop-->
  <LogState>Develop</LogState>
  <!--日志最大容量-->
  <LogMaxCapacity>2000</LogMaxCapacity>
  <!--日志的缓存处理-->
  <LogBufferNumber>1</LogBufferNumber>

  <!--其他系统额核心参数-->
</SystemConfigInfo>